  "description": "Auth package against Keycloak for Saml and Openid-Connect",
  "keywords": [
    "sngular",
    "auth",
    "keycloak",
    "saml",
    "onelogin",
    "connect"
  ],
  "homepage": "https://www.sngular.com/",
  "license": "MIT",
  "authors": [
    {
      "name": "Manuel Jesús García Ruiz",
      "email": "mjgr0013@gmail.com",
      "homepage": "http://seld.be",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "Sngular\\Auth\\": "src/"
    }
